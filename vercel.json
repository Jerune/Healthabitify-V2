{
  "buildCommand": "npm run build",
  "outputDirectory": ".next",
  "framework": "nextjs",
  "installCommand": "npm install",
  "devCommand": "npm run dev",
  "regions": ["iad1"],
  "functions": {
    "src/app/api/oura/auth/route.ts": {
      "maxDuration": 30
    },
    "src/app/api/oura/callback/route.ts": {
      "maxDuration": 30
    },
    "src/app/api/oura/[...path]/route.ts": {
      "maxDuration": 30
    },
    "src/app/api/fitbit/auth/route.ts": {
      "maxDuration": 30
    },
    "src/app/api/fitbit/callback/route.ts": {
      "maxDuration": 30
    },
    "src/app/api/fitbit/[...path]/route.ts": {
      "maxDuration": 30
    }
  }
}
